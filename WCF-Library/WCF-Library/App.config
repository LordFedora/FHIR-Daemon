    </services>
    <behaviors>
      <endpointBehaviors>
        <behavior name="WCF_Library.Feed1Behavior">
          <webHttp/>
        </behavior>
      </endpointBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>
