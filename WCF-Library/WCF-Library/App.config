        <endpoint address="Feed1" behaviorConfiguration="WCF_Library.Feed1Behavior"
          binding="webHttpBinding" contract="WCF_Library.IService1" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8733/Design_Time_Addresses/WCF_Library/" />
